{
  "name": "ordered-object-literal",
  "version": "0.0.0",
  "description": "Object literal maintaining its properties in the order they were added",
  "keywords": [
    "object",
    "object-literal",
    "iteration"
  ],
  "homepage": "https://github.com/P0lip/ordered-object-literal",
  "bugs": {
    "url": "https://github.com/P0lip/ordered-object-literal/issues"
  },
  "engines": {
    "node": ">=12"
  },
  "files": [
    "src/index.d.ts",
    "src/index.cjs",
    "src/index.mjs"
  ],
  "typings": "./src/index.d.ts",
  "main": "./src/index.cjs",
  "exports": {
    "import": "./src/index.mjs",
    "require": "./src/index.cjs"
  },
  "license": "MIT",
  "author": "P0lip",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/P0lip/ordered-object-literal.git"
  },
  "scripts": {
    "build": "NODE_ENV=production rollup -c",
    "lint": "eslint --cache --cache-location .cache/ src/**/*.mjs",
    "test": "NODE_ENV=test mocha --experimental-modules --config .mocharc ./src/__tests__/*.mjs",
    "prepublish": "yarn lint && yarn test && yarn build"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-chai-expect": "2.1.0",
    "eslint-plugin-chai-friendly": "0.5.0",
    "eslint-plugin-prettier": "3.1.2",
    "husky": "4.2.3",
    "mocha": "7.1.1",
    "prettier": "1.19.1",
    "rollup": "2.1.0"
  }
}
